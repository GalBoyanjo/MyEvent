<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.gal.invitation.Excel"
    android:orientation="vertical"
    tools:showIn="@layout/activity_excel"
    android:id="@+id/excel_layout">



<Button
        android:id="@+id/excel_export"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/export_excel"/>
    <Button
        android:id="@+id/excel_import"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/import_excel"/>

</LinearLayout>
